<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<DebugType>embedded</DebugType>
		<Authors>KnownChen</Authors>
		<Description>Known is a rapid development framework for .NET.</Description>
		<Version>1.2.17</Version>
		<PackageIcon>icon.png</PackageIcon>
		<PackageTags>known, framework, .net</PackageTags>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<PackageProjectUrl>https://gitee.com/known/Known</PackageProjectUrl>
		<PackageOutputPath>..\Library</PackageOutputPath>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\icon.png" />
		<None Update="..\icon.png">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

</Project>